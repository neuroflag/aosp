# zygote need to be started after otapreopt which will be done on post-fs-data
on zygote-start

    # for UART
    chmod 0666 /dev/ttysWK2
    chmod 0666 /dev/ttysWK3
    # for RS232
    chmod 0666 /dev/ttyS3
    chmod 0666 /dev/ttyS4
    # for RS485
    chmod 0666 /dev/ttysWK0
    chmod 0666 /dev/ttysWK1
