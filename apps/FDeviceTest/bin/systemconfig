#!/system/bin/sh
#copy /system/bin/

CLEAR_TEST="clear_test"


function clear_test()
{
  mount -o rw,remount /oem
  rm -rf /oem/devicetest
  sync
  rm -rf /sdcard/.LOGSAVE
  sync
  rm -rf /sdcard/gti
  sync
}

case "$1" in
       "$CLEAR_TEST")
               echo "system will get root..."
               clear_test
               ;;
       *) 
               echo "unexpected argument."
       ;;
esac
